<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
        Suppression file for OWASP Dependency Check
        This file contains suppressions for false positives and vulnerabilities
        that have been reviewed and accepted.
    -->
    
    <!-- DOMPurify CVE-2025-26791 in Swagger UI - Low severity, requires specific conditions -->
    <suppress>
        <notes><![CDATA[
        CVE-2025-26791 affects DOMPurify in Swagger UI.
        This is a low-severity XSS vulnerability that requires specific conditions.
        Swagger UI is only used for API documentation in development/testing.
        ]]></notes>
        <packageUrl regex="true">^pkg:javascript/DOMPurify@.*$</packageUrl>
        <cve>CVE-2025-26791</cve>
    </suppress>
    
    <!-- Elasticsearch REST client - These CVEs are for Elasticsearch server, not the Java client -->
    <suppress>
        <notes><![CDATA[
        These CVEs affect Elasticsearch server, not the Java REST client library.
        The elasticsearch-rest-client is just a HTTP client for connecting to Elasticsearch.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.elasticsearch\.client/elasticsearch\-rest\-client@.*$</packageUrl>
        <cve>CVE-2024-23444</cve>
        <cve>CVE-2024-23450</cve>
        <cve>CVE-2024-43709</cve>
        <cve>CVE-2024-52979</cve>
        <cve>CVE-2024-52981</cve>
        <cve>CVE-2023-49921</cve>
        <cve>CVE-2024-23445</cve>
        <cve>CVE-2024-23451</cve>
        <cve>CVE-2024-52980</cve>
        <cve>CVE-2024-23449</cve>
    </suppress>
    
    <!-- Elasticsearch Java client - Server CVEs not applicable to client -->
    <suppress>
        <notes><![CDATA[
        These CVEs affect Elasticsearch server, not the Java client library.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/co\.elastic\.clients/elasticsearch\-java@.*$</packageUrl>
        <cve>CVE-2025-68384</cve>
        <cve>CVE-2025-37727</cve>
        <cve>CVE-2025-68390</cve>
    </suppress>

